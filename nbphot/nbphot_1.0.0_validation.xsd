<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="Types.xsd"/>
	<xs:element name="PhotoObject">
		<xs:annotation>
			<xs:documentation>Comment describing your root element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="IdentificationNumber"/>
				<xs:element ref="Title"/>
				<xs:element ref="Type" maxOccurs="unbounded"/>
				<xs:element ref="ShortDescription"/>
				<xs:element ref="Authenticity" maxOccurs="1"/>
				<xs:element ref="Condition"/>
				<xs:element ref="Purpose" maxOccurs="unbounded"/>
				<xs:element ref="Collection"/>
				<xs:element name="DigitalFile">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="File" type="thumb"/>
							<xs:element ref="Comment"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="DigitalRights"/>
				<xs:element name="Source">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="IdentificationNumber">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="URI">
											<xs:complexType>
												<xs:attribute name="sps_fixed" use="required"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="AnyOtherNumber">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="IdentificationNumber" type="link"/>
													<xs:element ref="Link"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element ref="Type"/>
							<xs:element ref="Title"/>
							<xs:element name="Description" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="StorageLocation">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Type" maxOccurs="1"/>
							<xs:element name="Name" type="xs:string"/>
							<xs:element name="Description" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CreationEvent">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="CreationDate"/>
							<xs:element ref="Type"/>
							<xs:element ref="Creator" maxOccurs="unbounded"/>
							<xs:element name="CreationPlace" type="location"/>
							<xs:element name="ObjectRelated">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="Type"/>
										<xs:element ref="Comment"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element ref="Technique"/>
							<xs:element ref="Material"/>
							<xs:element name="TechnicalDescription">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Dimension">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Pixels_X_axis"/>
													<xs:element name="Pixels_Y_axis"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="CharacteristicsOfPhotograph">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="ISO"/>
													<xs:element name="ExposureTime"/>
													<xs:element name="LensAperture"/>
													<xs:element name="FocalLength"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="CharacteristicsOfDigitization">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="DPIResolution"/>
													<xs:element name="ColorDepth"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Exif"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ReferenceTo">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Entity" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="EntityId" type="link"/>
										<xs:element name="Relation" type="term"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Subject" type="term" maxOccurs="unbounded"/>
							<xs:element name="Note" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="OtherRelatedEvents">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Entity" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="EntityId" type="link"/>
										<xs:element name="Relation" type="term"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="OtherIdentificationNumbers">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Value"/>
							<xs:element name="DateAssigned" type="date"/>
							<xs:element name="EntityAssigned">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="Link" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="IsReferencedBy">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Entity" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="Link"/>
										<xs:element name="RelationType" type="term"/>
										<xs:element name="Comment" type="xs:string"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="admin"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="IdentificationNumber">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="sps_fixed" use="required"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Title" type="xs:string"/>
	<xs:element name="StorageLocation" type="xs:string"/>
	<xs:element name="Comment" type="xs:string"/>
	<xs:element name="Type" type="term"/>
	<xs:element name="Authenticity" type="term"/>
	<xs:element name="Condition" type="term"/>
	<xs:element name="Collection" type="term"/>
	<xs:element name="ShortDescription" type="xs:string"/>
	<xs:element name="Technique" type="term"/>
	<xs:element name="Material" type="term"/>
	<xs:element name="CreationDate" type="date"/>
	<xs:element name="Creator" type="link"/>
	<xs:element name="Purpose" type="term"/>
	<xs:element name="DigitalRights" type="term"/>
	<xs:element name="Link" type="link"/>

  <!--TYPES-->

    <xs:element name="admin">
   		<xs:complexType>
   			<xs:sequence>
   				<xs:element name="id" type="xs:string"/>
   				<xs:element name="uri_id" type="xs:string"/>
   				<xs:element name="lang" type="xs:string" minOccurs="1"/>
   				<xs:element name="organization" type="xs:string"/>
   				<xs:element name="creator" type="xs:string"/>
   				<xs:element name="saved" type="xs:string"/>
   				<xs:element name="locked" type="xs:string"/>
   				<xs:element name="status" type="xs:string"/>
   				<xs:element name="versions">
   					<xs:complexType>
   						<xs:sequence>
   							<xs:element name="numOfVersions" minOccurs="0"/>
   							<xs:element name="webVersion" minOccurs="0"/>
   							<xs:element name="versionId"/>
   							<xs:element name="versionUser"/>
   							<xs:element name="versionDate"/>
   						</xs:sequence>
   					</xs:complexType>
   				</xs:element>
   				<xs:element name="read" type="xs:string"/>
   				<xs:element name="write" type="xs:string" maxOccurs="unbounded"/>
   				<xs:choice minOccurs="0" maxOccurs="unbounded">
   					<xs:element name="info" type="xs:string" minOccurs="0"/>
   					<xs:element name="translationFrom" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
   					<xs:element name="entityType" minOccurs="0"/>
   					<xs:element name="publicationEnable" type="xs:string" minOccurs="0"/>
   					<xs:element name="schemaVersion" minOccurs="0"/>
   					<xs:element name="refs" minOccurs="0">
   						<xs:complexType>
   							<xs:sequence>
   								<xs:element name="ref" minOccurs="0" maxOccurs="unbounded">
   									<xs:complexType>
   										<xs:attribute name="sps_type" use="required"/>
   										<xs:attribute name="sps_id" use="required"/>
   									</xs:complexType>
   								</xs:element>
   							</xs:sequence>
   						</xs:complexType>
   					</xs:element>
   					<xs:element name="refs_by" minOccurs="0">
   						<xs:complexType>
   							<xs:sequence>
   								<xs:element name="ref_by" minOccurs="0" maxOccurs="unbounded">
   									<xs:complexType>
   										<xs:attribute name="sps_type" use="required"/>
   										<xs:attribute name="sps_id" use="required"/>
   										<xs:attribute name="isUnpublished"/>
   									</xs:complexType>
   								</xs:element>
   							</xs:sequence>
   						</xs:complexType>
   					</xs:element>
   					<xs:element name="createdAt" minOccurs="0"/>
   					<xs:element name="lastModified" minOccurs="0"/>
   					<xs:element name="versionFrom" minOccurs="0"/>
   				</xs:choice>
   			</xs:sequence>
   		</xs:complexType>
   	</xs:element>
   	<xs:complexType name="location">
   		<xs:sequence>
   			<xs:element name="GeopoliticalHierarchy" type="term"/>
   			<xs:element name="Link" type="link"/>
   		</xs:sequence>
   	</xs:complexType>
   	<xs:complexType name="contactInfo">
   		<xs:sequence>
   			<xs:element name="Location" type="location" maxOccurs="unbounded"/>
   			<xs:element name="Address" type="xs:string" minOccurs="0"/>
   			<xs:element name="Telephone" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
   			<xs:element name="Fax" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
   			<xs:element name="Email" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
   		</xs:sequence>
   	</xs:complexType>
   	<xs:complexType name="richText">
   		<xs:simpleContent>
   			<xs:extension base="xs:string">
   				<xs:attribute name="sps_html" type="xs:string" use="required"/>
   			</xs:extension>
   			<!--xsd:attribute name="sps_new" type="xsd:string" use="required"/-->
   			<!--xsd:attribute name="sps_preview" type="xsd:string" use="required"/-->
   		</xs:simpleContent>
   	</xs:complexType>
   	<xs:complexType name="thumb">
   		<xs:simpleContent>
   			<xs:extension base="xs:string">
   				<xs:attribute name="sps_browse" type="xs:string" use="required"/>
   			</xs:extension>
   			<!--xsd:attribute name="sps_new" type="xsd:string" use="required"/-->
   			<!--xsd:attribute name="sps_preview" type="xsd:string" use="required"/-->
   		</xs:simpleContent>
   	</xs:complexType>
   	<xs:complexType name="term">
   		<xs:simpleContent>
   			<xs:extension base="xs:string">
   				<xs:attribute name="sps_id" type="xs:string" use="required"/>
   				<xs:attribute name="sps_vocabulary" type="xs:string" use="required"/>
